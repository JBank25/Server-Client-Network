{
    "version": 4,
    "vendor": {
        "conan": {}
    },
    "include": [
        "build/Debug/generators/CMakePresets.json",
        "build/Release/generators/CMakePresets.json"
    ],
    "configurePresets": [
        {
            "name": "release",
            "inherits": "conan-release",
            "cacheVariables": {
                "ENABLE_UNIT_TESTING": "OFF"
            }
        },
        {
            "name": "unit-tests",
            "inherits": "conan-release",
            "cacheVariables": {
                "ENABLE_UNIT_TESTING": "ON"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "unit-tests",
            "configurePreset": "unit-tests"
        },
        {
            "name": "production",
            "configurePreset": "release"
        }
    ]
}